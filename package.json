{
    "name": "@ioka-technologies/asyncapi-templates",
    "version": "1.0.0",
    "description": "AsyncAPI generator templates monorepo",
    "private": true,
    "workspaces": [
        "rust-server",
        "ts-client"
    ],
    "keywords": [
        "asyncapi",
        "template",
        "generator",
        "monorepo"
    ],
    "author": "Ioka Technologies",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ioka-technologies/asyncapi-template.git"
    },
    "bugs": {
        "url": "https://github.com/ioka-technologies/asyncapi-template/issues"
    },
    "homepage": "https://github.com/ioka-technologies/asyncapi-template#readme",
    "scripts": {
        "test": "npm run rust-server:test && npm run ts-client:test",
        "test:template": "npm run rust-server:test:template",
        "test:generated": "npm run rust-server:test:generated",
        "test:simple": "npm run rust-server:test:simple",
        "test:mqtt": "npm run rust-server:test:mqtt",
        "test:multi:allfeatures": "npm run rust-server:test:multi:allfeatures",
        "test:multi:noauth": "npm run rust-server:test:multi:noauth",
        "test:multi": "npm run rust-server:test:multi",
        "test:examples": "npm run rust-server:test:examples",
        "test:integration": "npm run rust-server:test:integration",
        "lint": "npm run rust-server:lint",
        "lint:fix": "npm run rust-server:lint:fix",
        "clean": "npm run rust-server:clean && npm run ts-client:clean",
        "rust-server:test": "npm run test --workspace=rust-server",
        "rust-server:test:template": "npm run test:template --workspace=rust-server",
        "rust-server:test:generated": "npm run test:generated --workspace=rust-server",
        "rust-server:test:simple": "npm run test:simple --workspace=rust-server",
        "rust-server:test:mqtt": "npm run test:mqtt --workspace=rust-server",
        "rust-server:test:multi:allfeatures": "npm run test:multi:allfeatures --workspace=rust-server",
        "rust-server:test:multi:noauth": "npm run test:multi:noauth --workspace=rust-server",
        "rust-server:test:multi": "npm run test:multi --workspace=rust-server",
        "rust-server:test:examples": "npm run test:examples --workspace=rust-server",
        "rust-server:test:integration": "npm run test:integration --workspace=rust-server",
        "rust-server:lint": "npm run lint --workspace=rust-server",
        "rust-server:lint:fix": "npm run lint:fix --workspace=rust-server",
        "rust-server:clean": "npm run clean --workspace=rust-server",
        "ts-client:test": "npm run test --workspace=ts-client",
        "ts-client:test:generate": "npm run test:generate --workspace=ts-client",
        "ts-client:clean": "npm run clean --workspace=ts-client"
    },
    "devDependencies": {
        "eslint": "^8.57.0",
        "prettier": "^3.2.5"
    }
}